{
  "manifest_version": 2,

  "name": "TimeM",
  "description": "This extension attempts to track time on websites",
  "version": "1.0",

  "background" :{
	"scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.1.js", "parse-1.2.19.min.js"]
    }
  ],
  "browser_action": {
	"default_title": "Tracker",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
	"cookies",
	"<all_urls>"
  ]
}